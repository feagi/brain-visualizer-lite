; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Brain-Visualizer"
config/description="User UI for FEAGI"
run/main_scene="res://BrainVisualizer/BrainVisualizer.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
boot_splash/bg_color=Color(2.06962e-06, 0.264005, 0.51313, 1)
boot_splash/fullsize=false
boot_splash/image="uid://bmiod0cp5k8ns"
config/icon="res://icon.svg"

[autoload]

FeagiCore="*res://addons/FeagiCoreIntegration/FeagiCore/FeagiCore.gd"
BV="*res://BrainVisualizer/BV.gd"

[debug]

gdscript/warnings/unsafe_cast=1

[display]

window/size/viewport_width=1280
window/size/viewport_height=720

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/FeagiCoreIntegration/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/FeagiCoreIntegration/": "red",
"res://addons/FeagiCoreIntegration/FeagiCore/LocalCache/": "yellow",
"res://addons/FeagiCoreIntegration/FeagiCore/Networking/": "teal"
}

[gui]

theme/custom="res://BrainVisualizer/UI/Themes/source_themes/source_dark.tres"

